{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": [true, "log"],
    "no-empty-interface": false,
    "ordered-imports": {
      "options": {
        "import-sources-order": "any",
        "named-imports-order": "any"
      }
    },
    "member-ordering": {
      "options": {
        "order": "any"
      }
    }
  }
}
